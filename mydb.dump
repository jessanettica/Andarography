INSERT INTO "experiences" VALUES(107,17734461251, FALSE,'San Francisco Craft Beer Festival: Holiday Edition - Evening Session','San Francisco','2015-11-21 18:00:00.000000','2015-11-21 21:30:00.000000','The SF Craft Beer Festival - “Holiday Edition” 
