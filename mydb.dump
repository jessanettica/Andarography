INSERT INTO "experiences" VALUES(106,16157441342, FALSE,'MOCK EVENTBRITE FESTIVAL','San Francisco','2015-11-23 13:00:00.000000','2015-11-26 16:00:00.000000','**This event is not real and only for example purposes** 
  
  
  
